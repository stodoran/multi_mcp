[run]
source = src
omit =
    tests/*
    .venv/*
    */site-packages/*
    src/cli.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
    @abc.abstractmethod
precision = 2

[html]
directory = htmlcov
